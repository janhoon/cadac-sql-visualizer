		"clsx": "^2.1.1",
		"lucide-react": "^0.503.0",
		"monaco-editor": "^0.52.2",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"tailwind-merge": "^3.2.0",
		"tailwindcss": "^4.1.4",
		"web-tree-sitter": "^0.25.3"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
