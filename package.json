{
	"name": "cadac-sql-visualizer",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "biome lint --write src/",
		"format": "biome format --write src/",
		"preview": "yarn run build && wrangler pages dev ./dist",
		"deploy": "yarn run build && wrangler pages deploy --branch main ./dist",
		"postinstall": "cp node_modules/@janhoon/tree-sitter-sql/tree-sitter-sql.wasm public/"
	},
	"dependencies": {
		"@janhoon/tree-sitter-sql": "^0.1.3",
